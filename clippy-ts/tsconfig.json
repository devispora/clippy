{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "target": "ES2020",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "module":"ESNext",
    "moduleResolution":"Node16",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}